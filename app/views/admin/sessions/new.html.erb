<div class="login_page">
  <div class="container">
      <%= form_tag admin_sessions_path, method:"post", class: 'form-signin' do %>
        <h1 class="text-center">Sign In</h1>
        <div>
          <%= label_tag 'email', nil, class: '' %>
          <%= text_field_tag :email, nil, placeholder: "Email", class: "form-control" %>
        </div>
        <div>
          <%= label_tag "password", nil, class: '' %>
          <%= password_field_tag :password, nil, placeholder: "Password", class: 'form-control' %>
        </div>
          <%= submit_tag "Log In", class: 'btn btn-primary btn-block' %>
      <% end %>
  </div>
</div>
