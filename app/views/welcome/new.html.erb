<h1>Create New Massage</h1>
<%= form_with model: @message, url: welcome_path, method: :post do |f| %>
  <div>
    <%= f.label :full_name %><br>
    <%= f.text_field :full_name %>
  </div>
  <div>
    <%= f.label :Email %><br>
    <%= f.email_field :email %>
  </div>
  <div>
    <%= f.label :Content %><br>
    <%= f.text_area :content %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
